#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/apps.py:13
msgid "PostgreSQL extensions"
msgstr "PostgreSQL 扩展。"

#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/fields/array.py:21
#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/forms/array.py:17
#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/forms/array.py:157
#, python-format
msgid "Item %(nth)s in the array did not validate: "
msgstr "数组中的第 %(nth)s 项无法校验。"

#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "嵌套数组必须是相同长度。"

#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/fields/hstore.py:16
msgid "Map of strings to strings/nulls"
msgstr "字符串到字符串/空的映射"

#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/fields/hstore.py:18
#, python-format
msgid "The value of \"%(key)s\" is not a string or null."
msgstr "\"%(key)s\" 的值不是字符串或者为空。"

#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/fields/jsonb.py:30
msgid "A JSON object"
msgstr "一个JSON对象"

#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/fields/jsonb.py:32
msgid "Value must be valid JSON."
msgstr "值必须是有效的JSON格式"

#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Could not load JSON data."
msgstr "不能加载JSON数据。"

#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/forms/hstore.py:18
msgid "Input must be a JSON dictionary."
msgstr "输入必须是JSON字典。"

#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/forms/jsonb.py:20
#, python-format
msgid "'%(value)s' value must be valid JSON."
msgstr "'%(value)s' 值必须是有效的JSON格式"

#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/forms/ranges.py:13
msgid "Enter two valid values."
msgstr "输入两个有效的值。"

#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/forms/ranges.py:14
msgid "The start of the range must not exceed the end of the range."
msgstr "区间开头不能超过区间结尾。"

#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/forms/ranges.py:61
msgid "Enter two whole numbers."
msgstr "输入两个整数。"

#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/forms/ranges.py:67
msgid "Enter two numbers."
msgstr "输入两个数字。"

#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/forms/ranges.py:73
msgid "Enter two valid date/times."
msgstr "输入两个有效的日期／时间。"

#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/forms/ranges.py:79
msgid "Enter two valid dates."
msgstr "输入两个有效日期。"

#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/validators.py:31
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "某些键缺失：%(keys)s"

#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/validators.py:32
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "包含未知的键：%(keys)s"

#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/validators.py:76
#, python-format
msgid ""
"Ensure that this range is completely less than or equal to %(limit_value)s."
msgstr "请确保该区间内所有值少于或等于 %(limit_value)s 。"

#: nesaenv/lib/python2.7/site-packages/django/contrib/postgres/validators.py:82
#, python-format
msgid ""
"Ensure that this range is completely greater than or equal to "
"%(limit_value)s."
msgstr "请确保该区间内所有值大于或等于 %(limit_value)s 。"

